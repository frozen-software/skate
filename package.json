{
  "name": "skate",
  "productName": "skate",
  "version": "1.0.5",
  "description": "Browser swiftly and peacefully with Skate Browser",
  "main": "src/app.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src --color",
    "build-win": "electron-forge make --platform=win32",
    "build-mac": "electron-forge make --platform=darwin",
    "build-linux": "electron-forge make --platform=linux"
  },
  "keywords": [],
  "author": "Elijah",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "dmg",
          "zip"
        ],
        "linux": [
          "deb",
          "rpm",
          "zip"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "SkateBrowser",
        "iconUrl": "src/icons/skate_256x256.ico",
        "setupIcon": "src/icons/skate_256x256.ico",
        "loadingGif": "src/icons/loading_270.gif",
        "noMsi": false
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "skate"
      }
    }
  },
  "dependencies": {
    "contains-ads": "github:frozen-software/contains-ads",
    "electron-compile": "^6.4.4",
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.12.4"
  }
}
